{
  "name": "interview",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-create-react-app": "^2.1.1",
    "@storybook/react": "^5.3.18",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.2",
    "@testing-library/user-event": "^10.0.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.8",
    "@types/react": "^16.9.31",
    "@types/react-dom": "^16.9.6",
    "@types/react-helmet": "^5.0.15",
    "@types/react-router": "^5.1.5",
    "@types/react-router-dom": "^5.1.3",
    "customize-cra": "^0.9.1",
    "cz-conventional-changelog": "^3.1.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "ky": "^0.19.0",
    "ky-universal": "^0.5.0",
    "lint-staged": "^10.1.1",
    "node-sass": "^4.13.1",
    "prettier": "^2.0.2",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0-beta.2",
    "react-hot-loader": "^4.12.20",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-snap": "^1.23.0",
    "stylelint": "^13.2.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-no-indistinguishable-colors": "^1.3.0",
    "stylelint-rscss": "^0.4.0",
    "stylelint-scss": "^3.16.0",
    "typescript": "^3.8.3",
    "webpack-bundle-analyzer": "^3.6.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --coverage",
    "eject": "react-scripts eject",
    "build:analyze": "npm run build -- --analyze",
    "postbuild": "react-snap",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --fix ./src/**/*.{js,jsx,ts,tsx}",
    "lint:css": "stylelint --fix ./src/**/*.{css,scss}",
    "prettify": "prettier --write ./src/**/*.{css,scss,js,jsx,ts,tsx}",
    "commit": "git-cz",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "#(.*)$": "<rootDir>/src/$1",
      "^.+\\.scss$": "identity-obj-proxy"
    },
    "coverageDirectory": "<rootDir>/.internals/coverage",
    "setupFilesAfterEnv": [
      "<rootDir>/.internals/jest.js"
    ]
  },
  "babel": {
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "stylelint": {
    "extends": [
      "stylelint-config-recommended",
      "stylelint-rscss/config",
      "stylelint-a11y/recommended"
    ],
    "plugins": [
      "stylelint-scss",
      "stylelint-no-indistinguishable-colors",
      "stylelint-declaration-block-no-ignored-properties"
    ],
    "rules": {
      "plugin/stylelint-no-indistinguishable-colors": true,
      "plugin/declaration-block-no-ignored-properties": true
    }
  },
  "lint-staged": {
    "*.{css,scss,js,jsx,ts,tsx}": [
      "npm run prettify"
    ],
    "*.{css,scss}": [
      "npm run lint:css"
    ],
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:js"
    ]
  },
  "config-overrides-path": ".internals/override.js"
}
